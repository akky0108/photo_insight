{
  "name": "photo_insight (py310)",
  "build": {
    "dockerfile": "../Dockerfile",
    "context": "..",
    "target": "dev"
  },
  "workspaceFolder": "/workspaces/photo_insight",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspaces/photo_insight,type=bind",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-azuretools.vscode-docker"
      ],
      "settings": {
        "python.defaultInterpreterPath": "/usr/local/bin/python",
        "python.testing.pytestEnabled": true,
        "python.testing.pytestArgs": ["tests"],
        "python.analysis.extraPaths": ["/workspaces/photo_insight/src"]
      }
    }
  }
}